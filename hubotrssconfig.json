{
    "feeds": [{
        "id": "stackexchange",
        "name": "Stack Exchange",
        "request": {
            "uri": "https://civicrm.stackexchange.com/feeds",
            "headers": {
                "User-Agent": "civicrm-bolts (https://github.com/xurizaemon/bolts)"
            }
        },
        "room": "town square",
        "pingIntervalSeconds": "60",
        "alertPrefix": "",
        "alertSuffix": "",
        "messageTpl": "`New StackExchange question: [${latestItem.getTitle()}](${latestItem.getPermalink()})`"
    },
    {
        "id": "civicrm-blog",
        "name": "CiviCRM Blog",
        "request": {
            "uri": "https://civicrm.org/blog/feed",
            "headers": {
                "User-Agent": "civicrm-bolts (https://github.com/xurizaemon/bolts)"
            }
        },
        "room": "town square",
        "pingIntervalSeconds": "60",
        "alertPrefix": "",
        "alertSuffix": "",
        "messageTpl": "`New CiviCRM blog: [${latestItem.getTitle()}](${latestItem.getPermalink()})`"
    }]
}
